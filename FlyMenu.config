{
  "hotArea": {
    "edge": "top", /* top right left bottom of primary monitor*/
    "triggerHeight": 5, /* amount of pixels to trigger the menu*/
    "startPercentage": 15, /* only trigger the menu on some part of the screen edge start default=0  */
    "endPercentage": 45, /* only trigger the menu on some part of the screen edge end  default=100 */
    "catchMouse": true, /* stop mouse from sliding of the menu, most useful for edge=top to ensure the first menu item is selected */
    "catchHeight": 10 /* amount of pixels to reset the mouse to */
  },
"styling": {
"fontName": "Segoe UI",
"fontSize": 10
},
  "menuItems": [
    {
      "label": "Latest Desktop", /* shows the desktop you were last on (befor the current)*/
      "type": "switch before",
      "parameter": "",
      "icon": "preferences-desktop-wallpaper-2.ico"
    },
    {
      "label": "DESKTOP LIST", /* shows a list of all desktops to switch to */
      "type": "switch to",
      "parameter": "%id%"
    },
    {
      "label": "Next Desktop", /* switches to the next desktop with rollover from last to first */
      "type": "switch right",
      "parameter": "",
      "icon": "preferences-system-windows-2.ico"
    },
    {
      "label": "Prev Desktop", /* switches to the previous desktop with rollover from first to last */
      "type": "switch left",
      "parameter": "",
      "icon": "preferences-system-windows-2.ico"
    },
    {
      "label": "Desktop",
      "type": "shortcut",
      "parameter": "ModifiedKeyStroke(LWIN, VK_D)",
      "icon": "preferences-desktop-wallpaper-5.ico"
    },
    {
      "label": "PrintScreen",
      "type": "run",
      "parameter": "sharex -workflow \"Capture region\"",
      "icon": "image-x-generic.ico"
    },
    {
      "label": "PrintScreen Edit",
      "type": "run",
      "parameter": "sharex -workflow aufnahme",
      "icon": "kpaint.ico"
    },
    {
      "label": "More",
      "type": "shortcut",
      "parameter": "ModifiedKeyStroke(LCONTROL LMENU, VK_N)", /* ModifiedKeyStroke(LWIN LSHIFT, VK_T) First space separated list of Modifiers KOMMA list of Keys Keynames matches https://github.com/GregsStack/InputSimulatorStandard */
      "icon": "gdm.ico"
    },
    {
      "label": "Win+Tab",
      "type": "shortcut",
      "parameter": "ModifiedKeyStroke(LWIN, TAB)",
      "icon": "kllckety.ico"
    },
    {
      "label": "Win+H",
      "type": "shortcut",
      "parameter": "ModifiedKeyStroke(LWIN, VK_H)",
      "icon": "preferences-plugin.ico"
    },
    {
      "label": "OCR",
      "type": "shortcut",
      "parameter": "ModifiedKeyStroke(LWIN LSHIFT, VK_T)",
      "icon": "preferences-plugin.ico"
    },
    {
      "label": "F3",
      "type": "shortcut",
      "parameter": "KeyPress(F3)"
    },
    {
      "label": "exit",
      "type": "exit",
      "parameter": ""
    }
  ]
}